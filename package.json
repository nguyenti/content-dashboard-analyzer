{
  "name": "cwc-data-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Social Media Analytics Dashboard - Monorepo",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "start": "npm-run-all --parallel start:*",
    "start:client": "cd client && npm run preview",
    "start:server": "cd server && npm run start",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "cd client && rm -rf node_modules dist",
    "clean:server": "cd server && rm -rf node_modules dist",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "cd client && npm run lint",
    "lint:server": "cd server && npm run lint",
    "typecheck": "npm run typecheck:client && npm run typecheck:server",
    "typecheck:client": "cd client && npm run typecheck",
    "typecheck:server": "cd server && npm run typecheck"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "social-media",
    "analytics",
    "dashboard",
    "react",
    "express",
    "supabase"
  ],
  "author": "",
  "license": "MIT"
}